{
	"name": "Agoric Power Box",
	"version": "0.0.1",
	"description": "Your browser's gateway to the Agoric platform",
	"homepage_url": "https://github.com/Agoric/powerbox",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "powerbox@agoric.com",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage"
	],
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	}
}
